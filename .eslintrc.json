{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-essential"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "vue/component-definition-name-casing": "off",
      "vue/require-name-property": "off"
    }
  }
  